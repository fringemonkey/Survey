{
  "name": "tlc-survey",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "npm run build && wrangler pages dev dist --d1=DB=tlc-survey-db",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy dist --project-name tlc-survey",
    "deploy:sandbox": "npm run build && wrangler pages deploy dist --project-name tlc-survey --branch dev",
    "deploy:preview": "npm run build && wrangler pages deploy dist --project-name tlc-survey --branch preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:functions": "vitest functions",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "wrangler": "^4.54.0"
  }
}
